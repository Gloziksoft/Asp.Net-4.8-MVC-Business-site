    <meta name="copyright" content="Copyright (c) Gloziksoft, s.r.o. All rights reserved." />
    <meta name="Keywords" content="softvér, analýza, návrh, design, dizajn, vývoj, programovanie, web, aplikácie" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="language" content="sk" />
    <meta name="content-language" content="sk" />
    <meta name="author" content="Gloziksoft - www.gloziksoft.sk" />
    <meta name="robots" content="noodp" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="@Url.Content("~/images/contact.png")" />
    <link href="@Url.Content("~/Content/css/bootstrap.min.css?20171007")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/bootstrap-theme.min.css?20170121")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/site.min.css?ver1")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/font-awesome.min.css")" rel="stylesheet">

    <script type="text/javascript" src='@Url.Content("~/Content/js/jquery-3.4.1.min.js")'></script>
    <script type="text/javascript" src='@Url.Content("~/Content/js/jquery.unobtrusive-ajax.min.js")'></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/Pgsoftweb.GoogleAnalytics-1.min.js")"></script>
